<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "mybatis-3-mapper.dtd">
<mapper namespace="comCd">

    <select id="selectComCdList">
        SELECT  
                COM_CD
              , COM_CD_NM
        FROM    COM_CODE 
        WHERE   1 = 1
        AND     DISP_YN = 'Y'
        AND     DEL_YN = 'N'
        ORDER   BY  SEQ
    </select>

</mapper>